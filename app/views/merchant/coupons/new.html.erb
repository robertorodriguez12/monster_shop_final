<%= form_for [:merchant, @coupon] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :code %>
  <%= f.text_field :code %>
  <%= f.label :percent_off %>
  <%= f.text_field :percent_off %>
  <%= f.label :quantity_requirement %>
  <%= f.text_field :quantity_requirement %>
  <%= f.submit %>
<% end %>
